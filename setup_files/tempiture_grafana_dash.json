    {
      "content": "<SCRIPT>\nvar myGloc = window.location.protocol + \"//\" + window.location.hostname + \":5000\";\nvar myGSet = myGloc + \"/setgas/\";\nvar myGMove = myGloc + \"/movegas/\";\n</SCRIPT>\n<CENTER>\n<TABLE>\n<TR><TD colspan=6><CENTER><H2>Set Gas Level</H2></CENTER></TD></TR>\n<TR>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGSet + '0');\">0%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGSet + '25');\">25%</button></TD>\n<TD colspan=2><CENTER><button style=\"color:black\" onclick=\"$.ajax(myGSet + '50');\">50%</button></CENTER></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGSet + '75');\">75%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGSet + '100');\">100%</button></TD>\n</TR>\n<TR><TD colspan=6><CENTER><H2>Move Gas Level</H2></CENTER></TD></TR>\n<TR>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '-25');\">-25%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '-10');\">-10%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '-1');\">-1%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '1');\">+1%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '10');\">+10%</button></TD>\n<TD><button style=\"color:black\" onclick=\"$.ajax(myGMove + '25');\">+25%</button></TD>\n</TABLE>\n</CENTER>\n\n\"\"",
